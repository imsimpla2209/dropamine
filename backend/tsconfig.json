{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": "./src",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
     "lib": [
      "esnext"
    ],
    "skipLibCheck": true
  },
  "include": [
    "../backend/src"
  ],
   "exclude": [
    "node_modules",
    "dist"
  ],
}